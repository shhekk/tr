    "format:write": "pnpm format --write",
    "tsc": "pnpm --parallel -r tsc",
    "lint": "pnpm --parallel -r lint",
    "build": "pnpm --parallel -r build",
    "dev": "pnpm --parallel -r dev",
    "serve": "nx run-many -t serve",
    "validate": "pnpm format:write && pnpm tsc && pnpm lint",
    "api:dev": "nx run api:serve",
    "web:dev": "nx run web:serve",
    "api:build": "nx run api:build",
    "web:build": "nx run web:build",
    "prisma:migrate": "cd libs/backend/src/database && pnpx prisma migrate dev",
    "prisma:generate": "cd libs/backend/src/database && pnpx prisma generate",
    "prisma:dbpush": "cd libs/backend/src/database && pnpx prisma db push",
    "prisma:studio": "cd libs/backend/src/database && pnpx prisma studio"
